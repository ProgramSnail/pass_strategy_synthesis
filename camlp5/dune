  (run
   mkcamlp5.opt
   -package
   camlp5,camlp5.pa_o,camlp5.macro,camlp5.pr_dump,logger.syntax
   -package
   logger.syntax,GT.syntax,GT.syntax.all,ostap.syntax
   -o
   %{targets})))
